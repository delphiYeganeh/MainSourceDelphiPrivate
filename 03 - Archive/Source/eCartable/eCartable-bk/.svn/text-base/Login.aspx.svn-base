<%@ page language="C#" autoeventwireup="true" inherits="Cartable.UI.Login" CodeFile="Login.aspx.cs" theme="Default" enabletheming="true" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">

    <title>اتوماسیون اداری يگانه</title>
</head>
<body>
    <div class="container-login">
        <table class="table-login" width="425" height="212" border="0" cellpadding="0" cellspacing="0">
            <tr>
                <td colspan="5" background="App_Themes/<%= Page.Theme %>/images/login_08_01.gif" width="425" height="31">
                    <div class="company-name">
                        نام شرکت</div>
                </td>
            </tr>
            <tr>
                <td colspan="5" background="App_Themes/<%= Page.Theme %>/images/login_08_02.gif" width="425" height="11">
                </td>
            </tr>
            <tr>
                <td background="App_Themes/<%= Page.Theme %>/images/login_08_07.gif" width="13" height="153" class="style2">
                </td>
                <td background="App_Themes/<%= Page.Theme %>/images/login_08_04.gif" width="204" height="152">
                    <form id="form1" runat="server">
                        <div class="field-name">
                            <div class="content-main-form">
                                نام کاربري :</div>
                            <div class="textbox-align">
                                <asp:TextBox CssClass="textBox" ID="txtUsername" runat="server"></asp:TextBox>
                            </div>
                        </div>
                        <div class="field-name">
                            <div class="content-main-form">
                                رمز عبور :</div>
                            <div class="textbox-align">
                                <asp:TextBox CssClass="textBox" ID="txtPassword" TextMode="Password" runat="server"></asp:TextBox>
                            </div>
                        </div>
                   
                        <div class="field-name-button">
                            <div>
                                  <asp:ImageButton CssClass="button2" ID="btnLogin" runat="server" SkinID="btnLogin"
                                    AlternateText="Login" OnClick="btnLogin_Click" />
                            </div>
                        </div>
                             <div class="field-name-error">
                            <div>
                                <asp:Label  cssclass="error-login" ID="lblMsg" runat="server"></asp:Label>
                            </div>
                        </div>
                    </form>
                </td>
                <td background="App_Themes/<%= Page.Theme %>/images/login_08_05.gif" width="19" height="153">
                    &nbsp;</td>
                <td background="App_Themes/<%= Page.Theme %>/images/login_08_06.gif" width="157" height="153">
                    &nbsp;</td>
                <td background="App_Themes/<%= Page.Theme %>/images/login_08_03.gif" width="13" height="153">
                </td>
            </tr>
            <tr>
                <td colspan="5" background="App_Themes/<%= Page.Theme %>/images/login_08_08.gif" width="425" height="17">
                    &nbsp;</td>
            </tr>
        </table>
    </div>
    <div class="footer-login">
        Copyright © 2003-2007 Yeganeh soft .
    </div>
</body>
</html>
