  object QSessions: TADOQuery
    Connection = Dm.YeganehConnection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT wsb.*,wsb.BeginTimeHour+'#39':'#39'+wsb.BeginTimeMinute AS '#39'Begin' +
        'Time'#39',wsb.EndTimeHour+'#39':'#39'+wsb.EndTimeMinute AS '#39'EndTime'#39',wss.*,w' +
        'sv.*,wst.* FROM WorkSheet_Base wsb'
      'LEFT JOIN WorkSheet_Sessions wss ON wsb.ID=wss.WorkSheetBaseID'
      'LEFT JOIN WorkSheet_Tasks wst ON wsb.ID=wst.WorkSheetBaseID'
      'LEFT JOIN WorkSheet_Visits wsv ON wsb.ID=wsv.WorkSheetBaseID')
    Left = 536
    Top = 200
