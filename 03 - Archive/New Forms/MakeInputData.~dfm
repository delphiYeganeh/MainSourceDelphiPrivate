  object Fields: TADODataSet
    Connection = Dm.YeganehConnection
    CursorType = ctStatic
    CommandText = 
      'SELECT     Fields.*, FieldType.IsString, FieldType.IsLike, Field' +
      's.FieldTypeID, DERIVEDTBL.TableName reftable'#13#10'FROM         Field' +
      's INNER JOIN'#13#10'                      FieldType ON Fields.FieldTyp' +
      'eID = FieldType.FieldTypID LEFT OUTER JOIN'#13#10'                    ' +
      '      (SELECT     *'#13#10'                             FROM         T' +
      'ables'#13#10'                             WHERE     TableTypeID= 2) DE' +
      'RIVEDTBL ON Fields.RefrenceTableID = DERIVEDTBL.TableId'#13#10'WHERE  ' +
      '   (Fields.FieldTypeID <> 4)'#13#10'and Fields.tableid=:@tableid'
    Parameters = <
      item
        Name = '@tableid'
        DataType = ftWord
        Precision = 3
        Size = 1
        Value = 0
      end>
    Left = 80
    Top = 80
    object FieldsID: TAutoIncField
      FieldName = 'ID'
      ReadOnly = True
    end
    object FieldsFieldName: TWideStringField
      FieldName = 'FieldName'
      Size = 50
    end
    object FieldsTableID: TIntegerField
      FieldName = 'TableID'
    end
    object FieldsDescription: TWideStringField
      FieldName = 'Description'
      Size = 50
    end
    object FieldsIsWhere: TBooleanField
      FieldName = 'IsWhere'
    end
    object FieldsEveryLike: TBooleanField
      FieldName = 'EveryLike'
    end
    object FieldsVisibleInGrid: TBooleanField
      FieldName = 'VisibleInGrid'
    end
    object FieldsFieldGroupID: TWordField
      FieldName = 'FieldGroupID'
    end
    object Fieldsorder: TWordField
      FieldName = 'order'
    end
    object FieldsFieldTypeID: TWordField
      FieldName = 'FieldTypeID'
    end
    object FieldsRefrenceTableID: TWordField
      FieldName = 'RefrenceTableID'
    end
    object FieldsIsString: TBooleanField
      FieldName = 'IsString'
    end
    object FieldsIsLike: TBooleanField
      FieldName = 'IsLike'
    end
    object Fieldsreftable: TWideStringField
      FieldName = 'reftable'
      Size = 50
    end
  end
