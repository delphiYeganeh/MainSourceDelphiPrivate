  object TempAllData: TADOQuery
    Connection = Dm.YeganehConnection
    Parameters = <>
    SQL.Strings = (
      'SELECT c.[ID]'
      '      ,c.[ParentID]'
      '      ,c.[ChildID]'
      '      ,c.[OrderId]'
      
        '      ,CASE WHEN c.ChildID>0 THEN a.[ActionTypeTitle] ELSE  c.[D' +
        'escription] END [Description]'
      '      ,c.[Level]'
      '      ,c.[Type]'
      '      ,isnull(p.[Description],'#39#1588#1585#1705#1578#39') parentDescription'
      '      ,p.ParentID pparentid'
      '  FROM [dbo].[ActionType_Level] c'
      '  LEFT JOIN [dbo].[ActionType_Level] p ON c.ParentID = p.ID'
      '  LEFT JOIN [dbo].[ActionType] a ON a.ActionTypeID = c.ChildID')
    Left = 565
    Top = 13
    object TempAllDataId: TIntegerField
      FieldName = 'Id'
    end
    object TempAllDataParentID: TIntegerField
      FieldName = 'ParentID'
    end
    object TempAllDataChildID: TIntegerField
      FieldName = 'ChildID'
    end
    object TempAllDataOrderId: TIntegerField
