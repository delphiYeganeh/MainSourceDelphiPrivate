  object Select_Letter: TADOQuery
    Connection = Dm.ArchiveConnection
    CursorType = ctStatic
    CommandTimeout = 300
    Parameters = <
      item
        Name = 'p1'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      
        'select Letter.*,FromOrganizations.title senderTitle,Users.Title ' +
        'AS UserTitle , Classifications.Title AS ClassificTitle'
      ' from '
      '   Letter left JOIN'
      
        '                      FromOrganizations ON Letter.ToOrgID = From' +
        'Organizations.ID'
      'INNER JOIN users ON Users.ID = Letter.UserID'
      
        'INNER JOIN Classifications ON Classifications.Id = Classificatio' +
        'nID'
      ''
      'WHERE '
      #9'LetterID= :p1')
